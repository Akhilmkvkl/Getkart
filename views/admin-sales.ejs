    <div class="chart1" style="width: 70vw; height: 70vh; padding-left: 19vw;">
        <h3>Overall sales Details</h3>
        <canvas id="mysales" width="600" height="300" aria-label="chart" role="img"></canvas>
    </div>

    <script>

        const data = document.getElementsByClassName('sales')
        const key = document.getElementsByClassName('key')
        console.log(key)
        console.log('hii', data)
        let labels = [];
        let field = []
        for (x in data) {
            if (x <= data.length) {
                labels.push(data[x].value)

            }

        }
        for (x in key) {
            if (x <= data.length) {
                field.push(key[x].value)

            }

        }









        console.log(labels)
        console.log(field)


        const ctxx = document.getElementById('mysales').getContext('2d');
        const myCharts = new Chart(ctxx, {
            type: 'bar',
            data: {
                labels: field,
                datasets: [{
                    label: 'avg of sale',
                    data: labels,
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.2)',
                        'rgba(54, 162, 235, 0.2)',
                        'rgba(255, 206, 86, 0.2)',
                        'rgba(75, 192, 192, 0.2)',
                        'rgba(153, 102, 255, 0.2)',
                        'rgba(255, 159, 64, 0.2)'
                    ],
                    borderColor: [
                        'rgba(255, 99, 132, 1)',
                        'rgba(54, 162, 235, 1)',
                        'rgba(255, 206, 86, 1)',
                        'rgba(75, 192, 192, 1)',
                        'rgba(153, 102, 255, 1)',
                        'rgba(255, 159, 64, 1)'
                    ],
                    borderWidth: 1
                }]

            },
            options: {
                scales: {
                    y: {
                        beginAtZero: true
                    }
                }
            }
        });
    </script>

