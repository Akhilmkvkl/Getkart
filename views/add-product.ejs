    <% if(docs){ %>
        <div class="row justify-content-center mt-5">
            <div class="col-4">
                <form action="/adminhome/addproduct" method="post" enctype="multipart/form-data">

                    <div class="form-group">
                        <label for="exampleInputName1">Product Name</label>
                        <input type="name" name="name" class="form-control" id="name" value="<%= docs.name  %> ">
                    </div>
                    <div class="form-group mt-3">
                        <label for="exampleInputExpertise1">Price</label>
                        <input type="price" name="price" class="form-control" id="price" value="<%= docs.price  %> ">
                    </div>
                    <div class="form-group mt-3">
                        <label for="exampleInputDescription1">Description</label>
                        <input type="description" name="discription" class="form-control" id="description"
                            value="<%= docs.discription  %> ">
                    </div>
                    <div class="form-group  mt-3">
                        <label for="exampleInputFees1">Stock</label>
                        <input type="stock" name="stock" class="form-control" id="stock" value="<%= docs.stock  %> ">
                    </div>
                    <div class="form-group mt-3">
                        <select class="form-select" name="category" aria-label="Default select example">
                            <option selected class="category">Category</option>
                            <% for(let i=0;i<cat.length;i++){ %> 
                            <option value="<%= cat[i].category  %>"><%= cat[i].category  %> </option>
                            <% } %> 
                           
                        </select>
                    </div>
                    <div class="form-group mt-3">
                        <select class="form-select" name="subcategory" aria-label="Default select example">
                            <option selected class="subcategory" value="<%= docs.subcategory  %> ">Sub-Category</option>
                            <option value="male">male</option>
                            <option value="female">female</option>

                        </select>
                    </div>
                    <div class="row">
                        <div class="mt-3">
                            <input type="file" class="form-control" name="image1" accept="image/*" id="formFile"
                                multiple>
                        </div>
                        <div class="mt-3">
                            <input type="file" class="form-control" name="image2" accept="image/*" id="formFile"
                                multiple>
                        </div>

                        <button type="submit" class="btn btn-warning mt-3"
                            style="background-color: green !important;">Update Product</button>
                    </div>
                </form>
            </div>
        </div>

        <% }else{ %>

            <div class="row justify-content-center mt-5">
                <div class="col-4">
                    <form action="/adminhome/addproduct" method="post" enctype="multipart/form-data">

                        <div class="form-group">
